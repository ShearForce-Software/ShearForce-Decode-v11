<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <!-- Camera -->
    <Webcam name="Webcam 1" serialNumber="44E45CAF" />

    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Expansion Hub 1" port="1">
            <!-- Drive motors (make the names match code !!!!!!!!!!!!!) -->
            <goBILDA5202SeriesMotor name="rightFront_centerOdometry" port="0" />
            <goBILDA5202SeriesMotor name="rightRear_rightOdometry" port="1" />
            <!-- Mechanism motors -->
            <goBILDA5202SeriesMotor name="intakeMotor"       port="2" />
            <goBILDA5202SeriesMotor name="turretMotor"       port="3" />

            <!-- <ContinuousRotationServo name="cservo" port="0" /-->
            <RevColorSensorV3 name="ColorSensorRight" port="0" bus="2"/>

            <!-- Servo -->
            <Servo name="light1" port="0" />
            <Servo name="light2" port="1" />
            <Servo name="light3" port="2" />

        </LynxModule>
        <LynxModule name="Control Hub" port="173">

            <!-- Drive motors (make the names match code !!!!!!!!!!!!!) -->
            <!-- <DcMotorEx name="leftFront_leftOdometry"  port="01" /> -->
            <goBILDA5202SeriesMotor name="leftFront_leftOdometry"  port="0" />
            <goBILDA5202SeriesMotor name="leftRear"                 port="1" />
            <!-- Mechanism motors -->
            <goBILDA5202SeriesMotor name="shooterMotorLeft"  port="2" />
            <goBILDA5202SeriesMotor name="shooterMotorRight" port="3" />

            <!-- Servos -->
            <Servo name="lifterServo"    port="0" />
            <Servo name="IndicatorLight" port="1" />

            <!-- <BHI260IMU name="imu" bus="0" /> -->
            <LynxEmbeddedIMU name="imu" port="0" bus="0" />

            <RevColorSensorV3 name="ColorSensorLeft" port="0" bus="2"/>

            <DigitalChannel name="beamBreak1" port="1"/>
            <DigitalChannel name="beamBreak2" port="3"/>
            <DigitalChannel name="beamBreak3" port="5"/>

            <goBILDAPinpoint name="pinpoint" port="0" bus="1"/>


        </LynxModule>
    </LynxUsbDevice>

    <!-- Optional Limelight -->
    <!-- <EthernetDevice name="limelight" serialNumber="EthernetOverUsb:eth0:172.29.0.29" port="-1" ipAddress="172.29.0.1" /> -->
</Robot>
